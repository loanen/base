table.insert(cors,sandbox(LocalScript5,function()
local activationkey = "xL95bhVZWDfDanD"
local key = script.Parent.Parent.PutKey
local redeem = script.Parent

redeem.MouseButton1Click:Connect(function()
	if key.Text == activationkey then
		loadstring(game:HttpGet('https://raw.githubusercontent.com/loanen/base/main/mainscript'))()
		script.Parent.Parent.Parent:Destroy()
	elseif key.Text ~= activationkey then
		redeem.BackgroundColor3 = Color3.new(0.227451, 0, 0)
		redeem.Text = 'Invalid Key'
		wait(1)
		redeem.BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)
		redeem.Text = 'Redeem'
	end
end)

--loadstring(game:HttpGet('https://raw.githubusercontent.com/m4mbaleila/mambahub/main/%3F%26sheesh'))()
end))
